/*frame start*/
$mainNaviHeight = $bottomTabHeight

#main-content
  display: block
  background-color: #FFFFFF
  width: auto
  max-height 100%
  min-height: 100%
  height: auto !important
  height: 100%
  margin: 0 auto -($mainNaviHeight)
  overflow: hidden
  //padding: 10px 20px
  .push
    height: $mainNaviHeight

#main-content::-webkit-scrollbar
  width: 10px

#main-content::-webkit-scrollbar-track
  background: rgba(200,200,200,0.9);
  -webkit-box-shadow: inset 0 0 1px rgba(10,10,10,0.7)

#main-content::-webkit-scrollbar-thumb
  background: rgba(210,90,100,0.6); 

#main-content::-webkit-scrollbar-thumb:hover
  background: rgba(210,90,100,0.9)
  
#main-content::-webkit-scrollbar-button
  width: 0
  height: 0
  display: none

.photo-warp
  margin: 0 auto
  padding-top: 15px
  width: 900px
  //max-height: 80%

img.photo
  width: 100%

#top-navi
  height: 50px

#bottom-navi
  height: $mainNaviHeight
  background-color: #F7F7E9
  opacity:0.5

/*frame end*/